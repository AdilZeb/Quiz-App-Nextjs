{
  "name": "reactquiz",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "json-server": "json-server --watch db.json   port: 3002"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.16",
    "json-server": "^1.0.0-beta.3",
    "next": "15.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/vscode": "^1.95.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.generateCode",
        "title": "Generate Code with AI"
      }
    ],
    "keybindings": [
      {
        "command": "extension.generateCode",
        "key": "ctrl+shift+a",
        "when": "editorTextFocus"
      }
    ]
  }
}
